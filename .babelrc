{
  "presets": [
    ["@babel/env", {
      "target": {
        "browsers": ["> 1%", "last 2 versions"]
      },
      "modules": false,
      "loose": true,
      "useBuiltIns": "usage"
    }],
    ["@babel/stage-1", {
      "decoratorsLegacy": true
    }],
    "@babel/react"
  ],
  "plugins": [
    "@babel/transform-runtime",
    ["import", { "libraryName": "antd", "style": true }]
  ]
}
